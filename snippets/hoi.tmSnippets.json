  "Add event": {
    "prefix": "snippet_event",
    "body": [
      "country_event = {",
      "\tid = $1",
      "\ttitle = $2",
      "\tdesc = $3",
      "\tpicture = $4",
      "",
      "\tis_triggered_only = ${5|yes,no|}",
      "",
