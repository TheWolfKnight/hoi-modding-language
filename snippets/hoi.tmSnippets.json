  "Add object": {
    "prefix": "snippet_object",
    "body": [
      "$1 = {",
      "\t$2",
      "}$0"
    ]
  },

  "Add section comment": {
    "prefix": "snippet_section",
    "body": [
      "#######################################################################",
      "### SECTION: $1",
      "#######################################################################",
      "$0",
      "#######################################################################",
      "### END SECTION: $1",
      "#######################################################################",
    ]
  },

  "Add wargoal": {
    "prefix": "snippet_add_wargoal",
    "body": [
      "create_wargoal = {",
      "\ttype = ${1|annex_everything,puppet_wargoal_focus,take_state_focus|}",
      "\ttarget = ${2:COUNTRY_TAG}",
      "}"
    ]
  },

  "Add buildings": {
    "prefix": "snippet_add_buildings",
    "body": [
      "add_building_construction = {",
      "\ttype = ${1|industrial_complex,arms_factory,infrastructure,bunker,air_field|}",
      "\tlevel = $2",
      "\tinstant_build = ${3|yes,no|}",
      "}"
    ]
  },

  "Add State": {
    "prefix": "snippet_state",
    "body": [
      "state = {",
      "\tid=$1",
      "\tname=\"$2\"",
      "\tprovinces = {",
      "\t\t$3",
      "\t}",
      "\tmanpower=$4",
      "\tbuildings_max_level_factor=$5",
      "\tstate_category=${6|wasteland,enclave,tiny_island,pastoral,small_island,rural,town,large_town,city,large_city,metropolis,megalopolis|}",
      "\tlocal_supplies=$7",
      "\thistory={",
      "\t\t$8",
      "\t}\n}",
    ]
  }

