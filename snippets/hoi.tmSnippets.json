  "Add idea": {
    "prefix": "snippet_idea",
    "body": [
      "${1:idea_name} = {",
      "\tpicture = $2",
      "\tmodifier = {$3",
      "\t}",
      "",
      "\tallowed_civil_war = {$4",
      "\t}",
      "",
      "\ttargeted_modifier = {$5",
